{
  "name": "cspace-public-browser",
  "version": "0.0.1",
  "description": "CollectionSpace public browser",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collectionspace/cspace-public-browser.js.git"
  },
  "main": "src/index.jsx",
  "files": [
    "dist",
    "lib",
    "src",
    "styles",
    "images"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd",
    "build-cjs": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "build-umd": "npm run build-umd-dev && npm run build-umd-prod",
    "build-umd-dev": "webpack",
    "build-umd-prod": "cross-env NODE_ENV=production webpack",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf build lib dist coverage errorShots",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "devserver": "webpack-dev-server",
    "doc": "npx babel-node generateDocs.js",
    "lint": "eslint --ext .jsx,.js *.js src test examples",
    "prepare": "npm run clean && npm run build",
    "start": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --singleRun=true",
    "test-selenium": "wdio wdio.conf.js"
  },
  "dependencies": {
    "@appbaseio/reactivemaps": "^2.0.0-alpha-07",
    "@appbaseio/reactivesearch": "^2.7.0",
    "@types/googlemaps": "^3.0.0",
    "@types/markerclustererplus": "^2.1.29",
    "@types/react": "^16.0.0",
    "cspace-refname": "^1.0.0-rc.1",
    "history": "4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "jsonfile": "^4.0.0",
    "karma": "^2.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "loglevel": "^1.6.1",
    "mocha": "^5.2.0",
    "moxios": "^0.4.0",
    "nyc": "^11.5.0",
    "react-shallow-testutils": "^3.0.1",
    "react-test-renderer": "^16.3.1",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "^0.5.12",
    "wdio-sauce-service": "^0.4.8",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4",
    "webdriverio": "^4.12.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
